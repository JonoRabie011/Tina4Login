{
    "name": "tina4components/tina4login",
    "description": "This is a package will render a login screen and register screen that work with tin4sso",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "tina4stack/tina4php": "^2.0"
    },
    "scripts": {
        "tina4": "tina4",
        "tina4service": "tina4service",
        "test": "@tina4 tests:run",
        "test:verbose": "@tina4 tests:verbose",
        "initialize": "@tina4 initialize:run",
        "start": "@tina4 webservice:run",
        "start-service": "@tina4service",
        "post-autoload-dump": [
            "Tina4Login\\Tina4LoginInit::postPackageInstall"
        ]
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "loadModule.php"
        ],
        "psr-4": {
            "\\": [
                "src/",
                "src/app/",
                "src/orm/",
                "src/routes"
            ]
        }
    },
    "config": {
        "process-timeout": 0
    }
}